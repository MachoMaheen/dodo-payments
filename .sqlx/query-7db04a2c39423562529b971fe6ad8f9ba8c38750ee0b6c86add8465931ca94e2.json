  "query": "\n            SELECT id, sender_id, recipient_id, amount, currency, status as \"status: _\", created_at, updated_at\n            FROM transactions\n            WHERE (sender_id = $1 OR recipient_id = $1)\n            ORDER BY created_at DESC LIMIT $2 OFFSET $3\n            ",
